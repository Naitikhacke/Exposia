{
  "name": "exposia-backend",
  "version": "1.0.0",
  "description": "Exposia backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "tsx src/scripts/seed.ts",
    "studio": "prisma studio",
    "generate": "prisma generate"
  },
  "keywords": ["api", "social-network", "typescript"],
  "author": "Exposia Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "@prisma/client": "^5.8.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "socket.io": "^4.6.1",
    "aws-sdk": "^2.1524.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "sharp": "^0.33.1",
    "nodemailer": "^6.9.7",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
